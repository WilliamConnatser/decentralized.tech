{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "db_start_dev": "docker run --rm --name postgres_dev -d -p 6000:5432 postgres",
    "db_start_test": "docker run --rm --name postgres_test -d -p 7000:5432 postgres",
    "db_reset_dev": "npx knex migrate:rollback && npx knex migrate:latest",
    "db_reset_test": "npx knex migrate:rollback --env testing && npx knex migrate:latest --env testing",
    "db_stop_dev": "docker container stop postgres_dev",
    "db_stop_test": "docker container stop postgres_test",
    "dev": "npx cross-env NODE_ENV=development nodemon index.js",
    "test": "npx cross-env NODE_ENV=testing jest --watch --verbose"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.0",
    "cross-env": "^5.2.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.19.0",
    "pg": "^7.11.0",
    "ws": "^7.1.1"
  }
}
